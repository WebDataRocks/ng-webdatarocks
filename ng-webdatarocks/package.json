{
    "name": "ng-webdatarocks",
    "version": "1.4.3",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/WebDataRocks/ng-webdatarocks"
    },
    "keywords": [
        "pivot",
        "html5",
        "grid",
        "table",
        "pivottable",
        "pivotgrid",
        "pivotchart",
        "angular"
    ],
    "description": "Angular 4+ Module for WebDataRocks Web Pivot Table Tool",
    "private": false,
    "peerDependencies": {
        "@angular/core": ">=4.0.0",
        "@angular/common": ">=4.0.0"
    },
    "dependencies": {
        "webdatarocks": "1.4.3"
    },
    "ngPackage": {
        "$schema": "./node_modules/ng-packagr/ng-package.schema.json",
        "lib": {
            "entryFile": "./src/public_api.ts"
        },
        "dest": "../dist/ng-webdatarocks",
        "whitelistedNonPeerDependencies": [
            "webdatarocks"
        ]
    }
